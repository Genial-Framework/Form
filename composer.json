{
    "name": "genial-framework/form",
    "description": "Create elegant and reusable forms with complete ease.",
    "keywords": [
        "php72-component",
        "secure-by-default",
        "reusable",
        "form-builder"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas English",
            "email": "nenglish0820@outlook.com",
            "homepage": "https://github.com/Nenglish7"
        }
    ],
    "autoload": {
        "psr-4": {
            "Genial\\Form\\": "./src"
        }
    },
    "require": {
        "php": "^7.2"
    },
    "suggest": {
        "genial-framework/ajax": "For AJAX support."  
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vimeo/psalm": "^1"
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "prefer-stable": true
}
